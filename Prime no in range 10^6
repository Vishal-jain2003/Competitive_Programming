    


private static ArrayList<Integer> list = new ArrayList<>() {
        {
            boolean[] flag = new boolean[1000000];
            for (int i = 2; i < 1000000; i++) {
                if (!flag[i]) {
                    add(i);
                    for (int j = 2 * i; j < 1000000; j += i) {
                        flag[j] = true;
                    }
                }
            }
        }
    };
